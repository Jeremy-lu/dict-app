@import "weui.wxss";

/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  background-color: #f6f8f9;
} 

.contact {
  position: fixed;
  right: 5px;
  bottom: 5px;
  background-color:initial;
  border-color:initial;
  border:none;
  line-height:initial;
  padding:0;
 }

 .contact::after {
   content: none;
 }

 .contact image {
  width: 40px;
  height: 33px;
 } 
